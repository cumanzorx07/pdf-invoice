{
    "name": "cumanzorx07/pdf-invoice",
    "description": "Generar comprobante y facturas en PDF",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "keywords": ["invoice", "pdf", "factura", "comprobante"],
    "require": {
        "php": ">=5.6",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "setasign/fpdf": "~1.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": { "Cumanzorx07\\PdfInvoice\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Cumanzorx07\\PdfInvoice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
